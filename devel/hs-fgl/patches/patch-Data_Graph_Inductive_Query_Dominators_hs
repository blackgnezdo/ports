-{-# ANN iDom "HLint: ignore Use ***" #-}
 -- | return immediate dominators for each node of a graph, given a root
 iDom :: (Graph gr) => gr a b -> Node -> [(Node,Node)]
 iDom g root = let (result, toNode, _) = idomWork g root
